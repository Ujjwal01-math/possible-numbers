#include<stdio.h>
int func(int i,int num[],int v){
    int d=num[i]; 
    num[i]=num[v];
    num[v]=d;
    for(int i=0;i<=2;i++)
     printf("%d",num[i]);
}
int main(){
    int num[3],g=1;
    printf("Enter a number \n");
    for(int i=0;i<=2;i++){
     scanf("%d",&num[i]);
    } 
    printf("~~~~Here is the possible number of ways ~~~~\n");
    for(int i=0;i<=2;i++)
     printf("%d",num[i]);
    for(int i=0;i<=1;i++){
        printf("\n");
        int v=1+i;
        func(i,num,v);
    }
    for(int i=0;i<=1;i++){
        printf("\n");
        int v=1+i;
        func(i,num,v);
    }
    printf("\n");
    for(int i=0;i<=0;i++){
        func(i,num,g);
    }
}    
